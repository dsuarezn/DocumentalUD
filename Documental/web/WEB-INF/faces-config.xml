<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>loginController</managed-bean-name>
        <managed-bean-class>com.documental.beans.LoginController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>nivelAccesoController</managed-bean-name>
        <managed-bean-class>com.documental.beans.NivelAccesoController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index_MenuPrincipal</from-outcome>
            <to-view-id>/menuDocumental.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>tareaController</managed-bean-name>
        <managed-bean-class>com.documental.beans.TareaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>tipoUsuarioController</managed-bean-name>
        <managed-bean-class>com.documental.beans.TipoUsuarioController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>tuEtapaTareaController</managed-bean-name>
        <managed-bean-class>com.documental.beans.TipoUsuarioTareaController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
</faces-config>
